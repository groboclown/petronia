[run]

# __main__ is intentionally trivial so it doesn't need tests.
# Once the test generation for events makes the data-store event covered,
#   remove that coverage exclusion.
omit =
    petronia_extension_loader/__main__.py
    petronia_extension_loader/events/ext/datastore.py
    petronia_extension_loader/events/impl/extension_loader.py
    petronia_extension_loader/events/impl/foreman.py
    petronia_extension_loader/events/impl/tests/extension_loader_test.py
    petronia_extension_loader/events/impl/tests/foreman_test.py
